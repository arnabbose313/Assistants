pcm.!default {
  type asym
   playback.pcm {
     type plug
     slave.pcm "dmixer"
   }
   capture.pcm {
     type plug
     slave.pcm "dsnooper"
   }
}
pcm.dsnooper {
    type dsnoop
    ipc_key 816357492
    ipc_key_add_uid 0
    ipc_perm 0666
    slave {
        pcm "hw:seeed4micvoicec"
        channels 1
    }
}

pcm.mszeer {
  capture.pcm {
     type plug
     slave.pcm "dsnooper"
   }
  slave {
        pcm "hw:seeed4micvoicec"
        channels 1
    }
}
